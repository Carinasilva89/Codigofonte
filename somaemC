


#include <stdio.h>
#include <stdlib.h>

int main(int argc,char*argv[])
  {
   int numero1;
   int numero2;
   int soma;

   printf("digite um numero inteiro");
   scanf("%d",& numero1);
   printf("digite um numero inteiro");
   scanf("%d", & numero2);
   soma=numero1+numero2;
   printf("a soma e %d\n",soma);
    return 0;
}
