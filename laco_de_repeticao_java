
public class TestaLacos {
	
	public static void main(String[] args) {
		
		  int valor = 5;
		
		for(int multiplicador = 1; multiplicador <= 10; multiplicador ++ ) {
			int resultado = multiplicador * valor;
			
			System.out.println( valor + "x" + multiplicador + "=" + resultado);
		}
		    
		
	}

}

