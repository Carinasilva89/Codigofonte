#include <stdio.h>
#include <stdlib.h>

int main()
{
    int v[6];
    int cont;

    cont=0;
    for (int i=0; i<6; i++){
        printf ("Digite um valor:");
        scanf ("%d", &v[i]);
    }
    for (int i=0; i<6; i++){
        if (v[i]<0){
            cont=cont+1;}
    }
    printf("Seus Valores Negativos sao:%d\n",cont);
return 0;
}
